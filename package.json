{
  "name": "re-carousel",
  "version": "1.2.1",
  "description": "React carousel component, simplified.",
  "main": "carousel.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "babel src -d dist",
    "test": "eslint src && echo \"Error: no test specified\" && exit 1",
    "preexample": "npm run build && rm -rf ./gh-pages && cp -R dist gh-pages",
    "example": "cp example/*.html gh-pages && babel example -d gh-pages",
    "build:watch": "watch 'npm run build' src",
    "example:watch": "watch 'npm run example' example",
    "patch": "npm version patch -m 'v%s'",
    "minor": "npm version minor -m 'v%s'",
    "major": "npm version major -m 'v%s'",
    "preghp": "npm run example",
    "ghp": "gh-pages -d gh-pages -b gh-pages",
    "prepub": "npm run build",
    "pub": "cp dist/* ./ && npm publish && rm ./*.js"
  },
  "author": "Amio <amio.cn@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-standard": "^1.3.2",
    "gh-pages": "^0.11.0"
  },
  "keywords": [
    "react",
    "react-component",
    "carousel",
    "slider"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amio/re-carousel.git"
  },
  "bugs": {
    "url": "https://github.com/amio/re-carousel/issues"
  },
  "homepage": "https://github.com/amio/re-carousel#readme"
}
