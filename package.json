    "predeploy": "npm run build-pages",
    "deploy": "gh-pages -d build"
  },
  "files": [
    "dist/carousel.js"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
